#include <stdio.h>

int main() {
	int i,j,n;
	scanf("%d",&n);
	for(i=1;i<=n;i++)
	{   
	    char c='A';
	    if(i>1)
	    {
	        for(int k=1;k<i;k++)
	        c++;
	    }
	    for(j=1;j<=n;j++)
	    {
	        if(j>=i)
	        {printf("%c ",c);
	        c++;}
	        else
	        printf("  ");
	    }
	    printf("\n");
	}
	
	return 0;
}