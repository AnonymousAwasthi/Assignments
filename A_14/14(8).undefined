    #include <stdio.h>
    
    int main() {
    	int factorial(int n)
    	{
    	    int fact=1;
    	    if(n==0)
    	    return 1;
    	    else
        {
    	    for(int i=1;i<=n;i++)
                fact*= i;
                return fact;}
    	}
    	float nPr(int n,int r)
    	{
    	    return (float)factorial(n)/factorial(r);
    	}
    	float p=nPr(12,5);
    	printf("%.3f",p);
    	return 0;
    }